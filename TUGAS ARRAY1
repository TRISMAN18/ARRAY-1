#include <iostream>
using namespace std;
int nilai[5];
int n, hasil=0;
int main()
{
    for(int n=0;n<5;n++)
    {
        cout<<"masukkan nilai ke-"<<n(n+1)<<" : ";
        cin>>nilai[n];
    }
    cout<<endl;
   for(int n=0;n<5;n++)
    {
        hasil+=nilai[n];
        cout<<"hasil ke-"<<(n+1)<<" adalah : "<<hasil<<endl;
    }
   cout<<endl<<"jumlah seluruh nilai : "<<hasil<<endl;
   return 0;
   }
    
      
